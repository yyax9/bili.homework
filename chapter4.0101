public class BitOperator02 {

    public static void main(String[] args) {
        System.out.println(1 >> 2); 
        System.out.println(1 << 2);
        System.out.println(4 << 3);
        System.out.println(15 >> 2);

        System.out.println(-10.4&3);
        
        int i=66;
        System.out.println(++i+i);
    }
}
